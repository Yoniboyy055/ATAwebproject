{
  "name": "amannual-travel",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "prisma generate && next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "images:optimize": "node scripts/optimize-images.js",
    "images:optimize:dry": "node scripts/optimize-images-dryrun.js",
    "check:blurs": "node scripts/check-blurs.js",
    "check:variants": "node scripts/check-variants.js",
    "prebuild": "npm run check:blurs && npm run check:variants",
    "postinstall": "prisma generate",
    "db:push": "prisma db push",
    "db:reset": "prisma migrate reset --force",
    "db:seed": "node --require ts-node/register prisma/seed.ts",
    "prisma:generate": "prisma generate"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@builder.io/react": "^9.1.0",
    "@prisma/client": "^5.22.0",
    "@stripe/stripe-js": "^8.6.1",
    "bcryptjs": "^3.0.3",
    "lucide-react": "^0.562.0",
    "next": "^14.2.35",
    "next-auth": "^4.24.13",
    "prisma": "^5.22.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "resend": "^3.2.0",
    "stripe": "^20.1.2",
    "twilio": "^4.10.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/node": "^20.19.28",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "10.4.14",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.0.0",
    "postcss": "^8.5.6",
    "sharp": "^0.32.1",
    "tailwindcss": "3.4.8",
    "typescript": "5.3.3"
  }
}
